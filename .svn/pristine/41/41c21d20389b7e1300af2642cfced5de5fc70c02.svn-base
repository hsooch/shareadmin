/* layer popup  */

.layer-wrap:before {
	display: block;
	content: "";
	width: 100%;
	height: 100%;
	background-color: rgba(0, 0, 0, 0.2);
	position: fixed;
	top: 0;
	left: 0;
}

.layer-popup {
	position: absolute;
	top: 10%;
	left: 50%;
	transform: translate(-50%, 0%);
	width: 523px;
}

.layer-area {
	position: relative;
}

.btn-layer-x {
	position: absolute;
	top: 0;
	right: 0;
	background: url("../images/icon_x.png") no-repeat 50% 50%;
	width: 56px;
	height: 56px;
	border: 0;
	z-index: 100;
	cursor: pointer;
}

.layer-popup .layer-header {
	background-color: #0f4075;
	height: 114px;
	position: relative;
}

.layer-popup .layer-container {
	border: 1px solid #989898;
	border-top: 0;
	background-color: #fff;
}

.layer-popup .layer-header:before {
	position: absolute;
	content: "";
	display: block;
	width: 80px;
	height: 80px;
	background: url("../images/icon_popup.png") no-repeat 0 0;
	top: -40px;
	left: 50%;
	transform: translate(-50%, 0);
}

.tit-layer {
	color: #fff;
	font-size: 26px;
	font-family: "noto-medium";
	text-align: center;
	padding-top: 65px;
}

.layer-content {
	padding: 30px 20px 20px;
}

.layer-content .tx {
	font-size: 16px;
	text-align: center;
	font-family: "noto-regular";
	line-height: 1.3;
}

.layer-popup .confirm-box {
	width: 283px;
	border: 1px solid #d6d6d6;
	background-color: #fff;
	padding: 30px 26px;
	margin: 40px auto 0;
}

.layer-popup .tit-box {
	display: block;
	font-size: 20px;
	color: #333333;
	text-align: center;
	font-family: "noto-medium";
}

.layer-popup .confirm-box .icon-phone {
	display: block;
	width: 128px;
	height: 128px;
	background: url("../images/icon_phone.png") no-repeat 0 0;
	margin: 15px auto;
}

.layer-popup .confirm-box .btn-confirm {
	width: 100%;
	height: 60px;
	background-color: #0f4075;
	color: #fff;
}

.confirm-box .btn-confirm {
	border: 0;
	cursor: pointer;
}

.layer-container .btn-area {
	text-align: center;
}

.layer-container .btn-cancel {
	background-color: #989898;
	color: #fff;
	font-size: 20px;
	border: 0;
	width: 229px;
	height: 60px;
	cursor: pointer;
}

.message-box {
	width: 100%;
	padding: 25px 10px;
	border: 1px solid #d6d6d6;
	text-align: center;
	margin-top: 35px;
}

.message-box p.tx {
	font-size: 24px;
	color: #333;
	font-family: "noto-regular";
}

.message-box p.tx .tx-point {
	color: #0f4075;
}

.layer-content .tx-info {
	color: #999999;
	font-size: 16px;
	line-height: 1.3;
	margin-top: 10px;
	padding-left: 12px;
}

.layer-content .tx-info:before {
	content: "*";
	margin-left: -12px;
}

@media screen and (max-width: 768px) {
	.layer-popup {
		width: 288px;
	}
	.layer-popup .confirm-box {
		width: 211px;
	}
	.layer-popup .confirm-box .icon-phone {
		width: 96px;
		height: 96px;
		background-size: 96px 96px;
	}
	.tit-layer {
		font-size: 16px;
		padding-top: 37px;
	}
	.layer-popup .layer-header {
		height: 67.5px;
	}
	.layer-popup .layer-header:before {
		width: 50px;
		height: 50px;
		background-size: 50px 50px;
		top: -20px;
	}
	.btn-layer-x {
		width: 30px;
		height: 30px;
		background-size: 20px 20px;
	}
	.layer-content .tx {
		font-size: 14px;
	}
	.br {
		display: block;
	}
	.layer-popup .tit-box {
		font-size: 15px;
	}
	.layer-popup .confirm-box {
		text-align: center;
		margin-top: 22.5px;
	}
	.layer-popup .confirm-box .btn-confirm {
		height: 40px;
		width: 138.5px;
		margin: 0 auto;
	}
	.layer-container .btn-cancel {
		height: 40px;
		font-size: 16px;
	}
	.message-box {
		padding: 10px 5px;
		margin-top: 24px;
	}
	.message-box p.tx {
		font-size: 15px;
	}
	.layer-content .tx-info {
		font-size: 12px;
	}
}

/* //layer popup  */

/* select box list type */

.tit-label {
	display: inline-block;
	font-size: 16px;
	background-color: #d4d4d4;
	text-align: center;
	border: 1px solid #b2b2b2;
	width: 177px;
	height: 40px;
	line-height: 38px;
}

.select-ui {
	position: relative;
	width: 175px;
	text-align: left;
}

.select-ui.open ul {
	display: block;
	z-index: 1000;
}

.select-ui strong a {
	z-index: 10;
	position: relative;
	display: block;
	width: 100%;
	padding-left: 10px;
	height: 40px;
	font-size: 16px;
	font-weight: normal;
	line-height: 38px;
	color: #686c73;
	text-decoration: none;
	background: #fff url('../images/select_down.png') no-repeat 95% 50%;
	border: 1px solid #b2b2b2;
}

.select-ui strong a.on {
	background: url('../images/select_up.png') no-repeat 95% 50%;
}

.select-ui strong a:hover {
	text-decoration: none;
}

.select-ui ul {
	display: none;
	position: absolute;
	width: 100%;
	max-height: 200px;
	top: 40px;
	overflow: hidden;
	overflow-y: scroll;
	left: 0;
	border: 1px solid #b2b2b2;
	background: #fff;
	border-top: 0;
}

.select-ui ul li {
	display: inline;
	float: left;
	width: 100%;
	border-top: 1px solid #e7e7e7;
}

.select-ui ul li:first-child {
	border-top: 0;
}

.select-ui ul li a {
	display: block;
	height: 40px;
	line-height: 38px;
	padding-left: 10px;
	color: #686c73;
	font-size: 16px;
	text-decoration: none;
}

.select-ui ul li a:hover {
	color: #fff;
	text-decoration: none;
	background: #0f4075;
}

@media screen and (max-width:768px) {
	.select-ui strong a {
		font-size: 12px;
	}
	.select-ui ul{
		top:30px;
	}
	.select-ui ul li a {
		font-size: 12px;
	}
}

/* //select box list type */

/* custom select */

select.select {
	position:relative;
	height: 40px;
	padding-left: 10px;
	background: url("../images/icon_arrow_down.png") no-repeat 90% 50%;
	filter:alpha(opacity=0);
	-webkit-appearance: none;
	-moz-appearance: none; appearance: none;

}

select.select:before {
	content: "";
	position: absolute;
	top: 50%;
	right: 15px;
	width: 0;
	height: 0;
	margin-top: -1px;
	border-left: 5px solid transparent;
	border-right: 5px solid transparent;
	border-top: 5px solid #333;
}

@media screen and (max-width:768px) {
	select.select{
		height: 30px;
	}
}

/* list type table */
.tit-tb {
	width: 100%;
	max-width: 1200px;
	margin: 0 auto;
	font-size: 20px;
	font-family: "noto-regular"
}
@media screen and (max-width:768px) {
	.tit-tb{
		font-size:18px;
		padding:0 15px;
		margin-top:20px;

	}
}
.tb-list {
	width: 100%;
	max-width: 1200px;
	border: 1px solid #d4d4d4;
	table-layout: fixed;
}

.tb-list thead tr th {
	background-color: #d1e2f4;
	border-left: 1px solid #dddddf;
	padding: 19px 5px;
	text-align: center;
	font-family: "noto-medium";
	letter-spacing: -1px;
}

.tb-list thead tr th:first-child {
	border-left: 0;
}

.tb-list tbody tr td {
	border-left: 1px solid #dddddf;
	border-top: 1px solid #dddddf;
	text-align: center;
	padding: 10px 5px;
	color: #333;
}

.tb-list tbody tr td .tx-no-result {
	padding: 110px 5px;
	text-align: center;
	font-family: "noto-medium"
}

.tb-list tbody tr td:first-child {
	border-left: 0;
}

.inner-scroll {
	max-height: 266px;
	overflow-x: hidden;
	overflow-y: auto;
}
@media screen and (max-width:768px) {
	.tb-list thead tr th{
		font-size:14px;
	}
	.tb-list thead tr th:first-child {
		padding-left:0;
		padding-right:0;
	}
	.tb-list tbody tr td{
		font-size:14px;
		padding:7px 5px;
	}
}
