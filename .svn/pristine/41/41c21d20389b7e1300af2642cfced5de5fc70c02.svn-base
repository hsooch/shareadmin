/* button */

button[type="button"]{
	background:none;
	border:0;
	cursor: pointer;
	width: auto;
	padding:0;
}
.btn-area {
	margin-top: 50px;
}

.btn-area.center {
	text-align: center;
}

.btn-area input {
	border: 0;
	color: #fff;
	cursor: pointer;
	font-size: 20px;
}

a.btn-orange {
	display: inline-block;
	background-color: #ff9c00;
	border-radius: 0;
	height: 60px;
	line-height: 60px;
	padding: 0 10px;
	color: #fff;
	font-size: 20px;
}

a.btn-navy {
	display: inline-block;
	background-color: #0f4075;
	border-radius: 0;
	height: 60px;
	line-height: 60px;
	padding: 0 10px;
	color: #fff;
	font-size: 20px;
}

.btn-navy input {
	background-color: #0f4075;
	border-radius: 0;
	height: 60px;
	padding: 0 10px;
	min-width: 229px;
	border: 0;
	color: #fff;
	cursor: pointer;
	font-size: 20px;
}

.btn-gray input {
	background-color: #989898;
	border-radius: 0;
	height: 60px;
	padding: 0 10px;
	min-width: 229px;
}

.btn-orange input {
	background-color: #ff9c00;
	border-radius: 0;
	height: 60px;
	padding: 0 10px;
	min-width: 229px;
	border:0;
	color:#fff;
	cursor:pointer;
}
.btn-silver input{
	background-color: #d2d2d2;
	border-radius: 0;
	height: 60px;
	padding: 0 10px;
	min-width: 229px;
	border:0;
	color:#000;
	cursor:pointer;
}
.btn-sm input {
	min-width: 95px;
	height: 40px;
	font-size: 16px;
}

.btn-mini input {
	min-width: 76px;
	height: 32px;
	font-size: 16px;
}
.btn-step-icon{
	position:relative;
}
.btn-step-icon:after{
	position:absolute;
	right:20px;
	content:"〉";
	font-size:20px;
	color:#fff;
	top:0;
}
.btn-side{
	max-width:1200px;
	width:100;
	margin:20px auto 0;
}
.btn-side:after{
	content:"";
	display: block;
	clear:both;
}
.btn-side .fl-left{
	float:left;
}
.btn-side .fl-right{
	float:right;
}
.btn-side.top-insert{
	margin-top:-50px;
}
@media screen and (max-width:1200px){

}
@media screen and (max-width: 768px) {
	.btn-area {
		margin-top: 20px;
	}
	.btn-custom input {
		min-width: 138.5px;
		height: 40px;
		font-size: 16px;
	}
	.btn-mini input {
		min-width: 44.5px;
		height: 25px;
		font-size:14px;
	}
	.btn-step-icon:after{
		position:absolute;
		right:5px;
		content:"〉";
		font-size:14px;
		color:#fff;
		top:5px;
	}
	.btn-side.top-insert{
		margin-top:0
	}
}

/*paging*/

.paing-custom ._paging ._inner ._pageState, .paing-custom ._paging ._inner a._listNext, .paing-custom ._paging ._inner a._listPrev {
	display: none !important;
}

.paing-custom {
	line-height: 2;
	position: relative;
	height: auto;
	margin-top: 20px;
}

.paing-custom ._paging {
	padding: 0;
	text-align: center;
}

.paing-custom ._paging ._inner {
	height: auto;
	border: 1px solid #acb1b5;
	overflow: hidden;
	position: relative;
	height: 38px;
	display: inline-block;
	zoom: 1;
}

.paing-custom ._paging ._inner a, .paing-custom ._paging ._inner strong {
	width: 40px;
	height: auto;
	margin: 0;
	font-size: 16px;
	line-height: 18px;
	border: none;
}

.paing-custom ._paging ._inner a._listNext, .paing-custom ._paging ._inner a._listPrev {
	display: block !important;
}

.paing-custom ._paging ._inner>a {
	width: 40px;
	height: 40px;
	background-color: #fff;
	border: none;
}

.paing-custom ._paging ._inner>a._first {
	background: url(../images/prve.png) no-repeat center;
	border-right: 1px solid #acb1b5;
}

.paing-custom ._paging ._inner>a._last {
	background: url(../images/next.png) no-repeat center;
	border-left: 1px solid #acb1b5;
}

.paing-custom ._paging ._inner a._listNext {
	background: url(../images/next1.png) no-repeat center;
	border-left: 1px solid #acb1b5;
}

.paing-custom ._paging ._inner a._listPrev {
	background: url(../images/prev1.png) no-repeat center;
	border-right: 1px solid #acb1b5;
}

.paing-custom ._paging ._inner ul {
	height: auto;
	padding: 0 20px;
	overflow: hidden;
	position: relative;
	float: left;
}

.paing-custom ._paging ._inner ul li a {
	display: block !important;
	float: left;
	width: 40px;
	height: 100%;
	line-height: 32px;
}

.paing-custom ._paging ._inner ul li a strong {
	position: relative;
	background-color: #fff;
	color: #0f4075;
	font-weight: bold;
	line-height: 1;
	line-height: 32px;
}

.paing-custom ._paging ._inner ul li strong::after {
	position: absolute;
	left: 50%;
	bottom: 6px;
	margin-left: -4px;
	display: block;
	content: "";
	width: 8px;
	height: 1px;
	background-color: #0f4075;
}

.paing-custom ._paging ._inner ul li a {
	font-family: "noto-light";
	color: #767676;
	background-color: #fff;
}

.paing-custom ._paging ._inner a {
	text-align: center;
}

.paing-custom ._paging ._inner a, .paing-custom ._paging ._inner li, .paing-custom ._paging ._inner p, .paing-custom ._paging ._inner span, .paing-custom ._paging ._inner strong, .paing-custom ._paging ._inner ul {
	float: left;
	height: 100%;
}

.paing-custom ._paging ._inner a._first, .paing-custom ._paging ._inner a._last, .paing-custom ._paging ._inner a._listNext, .paing-custom ._paging ._inner a._listPrev, .paing-custom ._paging ._inner a._next, .paing-custom ._paging ._inner a._prev {
	text-indent: -5000px;
}

.paing-custom ._paging ._inner ._pageState {
	display: none;
	padding: 0 1em;
}

._fnctWrap ._paging ._inner a, ._fnctWrap ._paging ._inner strong {
	width: 40px;
	height: auto;
	margin: 0;
	font-size: 16px;
	line-height: 18px;
	border: none;
}

@media screen and (max-width: 768px) {
	/*paging*/
	.paing-custom{
		padding: 0;
	}
	.paing-custom ._paging ._inner{
		height:28px;
	}
	.paing-custom ._paging ._inner>a {
		width: 30px;
		height: 28px;
		line-height: 28px;
	}
	.paing-custom ._paging ._inner ul {
		padding: 0 15px;
	}
	.paing-custom ._paging ._inner a, .paing-custom ._paging ._inner strong {
		width: 30px;
		/* padding: 8px 0; */
		font-size: 12px;
		line-height: 14px;
	}
	.paing-custom ._paging ._inner ul li a{
		line-height: 26px;
		width:27px;
	}
	.paing-custom ._paging ._inner ul li a strong{
		line-height: 26px;
	}
	.paing-custom ._paging ._inner ul li a{}
}

/* 정보입력 form  */

.form-custom {
	max-width: 1200px;
	width: 100%;
	margin: 0 auto;
	border-top: 3px solid #75757f;
	border-left: 1px solid #bbbbbb;
	border-right: 1px solid #bbbbbb;
	letter-spacing: -0.5px;
}

.form-custom input[type="text"].input-text, .form-custom input[type="password"].input-text {
	width: 100%;
	height: 40px;
	border: 1px solid #dedede;
	padding-left: 20px;
	font-size: 16px;
}

.form-custom select.select {
	height: 40px;
	min-width: 100px;
	padding-left: 10px;
}

.input-tr {
	border-bottom: 1px solid #bbbbbb;
	display: table;
	width: 100%;
}

.input-tr .input-th {
	display: table-cell;
	vertical-align: middle;
	padding: 20px;
	width: 272px;
	text-align: center;
	background-color: #deeaf7;
	color: #333;
	height: 80px;
}

.input-tr .input-th label {
	font-family: "noto-medium";
	line-height: 1.3;
}

.input-tr .input-td {
	display: table-cell;
	vertical-align: middle;
	padding: 20px;
	height: 80px;
}

.input-td .cell-group {
	display: table;
	width: 100%;
}

.input-td .cell-group .cell {
	display: table-cell;
	vertical-align: middle;
	width: auto;
	min-width: 5%;
	padding-left: 4px;
}

.input-td .cell-group .cell:first-child {
	padding-left: 0;
	width: 30%;
}

.input-td .cell-group .cell.symbol {
	width: 20px;
	text-align: center;
}

.form-custom .btn-request {
	width: 132px;
	height: 40px;
	background-color: #989898;
	border: 0;
	color: #fff;
	font-size: 16px;
	cursor: pointer;
}

.tx-info {
	margin-top: 10px;
	line-height: 1.3;
}

.cell .tx-info {
	margin-top: 0;
	vertical-align: middle;
	margin-left: 10px;
}

.cell .tx-info span {
	vertical-align: middle;
}

.icon-essential:before {
	display: inline-block;
	content: "*";
	color: #ff0000;
	vertical-align: middle;
}

.input-tr .thtd-group {
	display: table-cell;
	vertical-align: middle;
	width: 50%;
}

.birth-group .select {
	width: 102px;
}

.number-group .select, .number-group input[type="text"].input-text {
	width: 102px;
}

.univ-col .input-th, .univ-col .input-td {
	height: 136px;
}

.input-tr .input-td {}

.univ-col .row {
	width: 100%;
	margin-top: 5px;
}

.univ-col .input-td.row-group:after {
	content: "";
	display: block;
	clear: both;
}

.univ-col .input-th {
	display: inline-block;
	float: left;
	vertical-align: middle;
	line-height: 90px;
}

.univ-col .input-td {
	float: left;
	display: inline-block;
	vertical-align: middle;
	line-height: 90px;
}

.univ-col .input-td.row-group {
	line-height: 1.3;
}

@media screen and (max-width: 768px) {
	.form-custom {
		min-width: 290px;
		border-left: 0;
		border-right: 0;
	}
	.form-custom input[type="text"].input-text, .form-custom input[type="password"].input-text {
		width: 100%;
		height: 25px;
		border: 1px solid #dedede;
		padding-left: 8px;
		font-size: 12px;
	}
	.form-custom .select {
		height: 24px;
		min-width: 55px;
		padding-left: 8px;
		font-size: 12px;
	}
	.form-custom select.select {
		height: 25px;
	}
	.input-tr .input-th label {
		font-size: 12px;
	}
	.input-tr .input-th {
		width: 100px;
		padding: 10px 2px;
		height: auto;
		min-height: 60px;
	}
	.input-tr .input-td {
		padding: 10px;
		padding-right: 0;
		font-size: 12px;
		height: auto;
		min-height: 60px;
		width: calc(100% - 100px)
	}
	.input-td .cell-group .cell {
		display: block;
		width: 100%;
		margin-top: 7px;
		padding-left: 0;
	}
	.input-td .cell-group .cell:first-child {
		margin-top: 0;
	}
	.input-td .cell-group .cell.symbol {
		display: inline-block;
		text-align: center;
		margin-top: 0;
		width: 40px
	}
	.input-td .cell-group .cell.m-insert {
		display: inline-block;
		width: calc(100% - 40px) !important;
	}
	.form-custom .btn-request {
		margin-left: 0;
		width: 177px;
		height: 25px;
		font-size: 12px;
	}
	.tx-info {
		font-size: 12px;
		padding-left: 10px;
	}
	.tx-info span {
		margin-left: -8px;
	}
	.cell .tx-info {
		padding-left: 0;
	}
	.input-td .cell-group .cell:first-child {
		width: 50%;
	}
	.input-tr .thtd-group {
		display: block;
		width: 100%;
		clear: both;
	}
	.input-tr .thtd-group .input-th, .input-tr .thtd-group .input-td {
		height: 45px;
	}
	.input-tr .thtd-group:first-child {
		border-bottom: 1px solid #bbbbbb;
	}
	.birth-group select.select {
		width: 55px;
		min-width: 55px;
	}
	.number-group select.select, .number-group input[type="text"].input-text {
		width: 55px;
		min-width: 55px;
	}
	.input-tr.univ-col .thtd-group .input-th {
		line-height: 65px;
	}
	.input-tr.univ-col .thtd-group .input-th, .input-tr.univ-col .thtd-group .input-td {
		height: auto;
		border-top: 1px solid #ddd;
	}
	.input-tr.univ-col .thtd-group.type2 .input-th, .input-tr.univ-col .thtd-group.type2 .input-td {
		height: 45px;
		line-height: 1.5;
		min-height: 45px;
	}
}

