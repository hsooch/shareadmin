/*전체 css 적용*/

._table > tbody > tr > td span._button{vertical-align: top;} 
._table > tbody > tr > td label {vertical-align: middle;line-height:36px;} 
._table > tbody > tr > td select{vertical-align: middle;}
.miniList tbody > tr > td label {padding:0 10px;}

/*
 _articleContent
 */

._articleContent table._table {
  border-spacing: 0;
  width: 100%;
  border-width: 2px 0 2px 0 ;
  border-style: solid;
  border-color: #07337c;
  table-layout: fixed;
  position: relative;
  }
._articleContent table._table tr{height: 50px;}
._articleContent table._table caption {
  display: none;
}

._articleContent table._table th {
  border-width: 0 1px 1px 0;
  border-style: solid;
  border-color: #d1d1d1;
  padding: 9px;
  text-align: left;
  word-break: break-all;

}
._articleContent table._table td {
  border-width: 0 1px 1px 0;
  border-style: solid;
  border-color: #d1d1d1;
  padding: 9px;
  text-align: left;
  overflow: hidden;
  word-break: break-all;
}

._articleContent table._table thead tr th {
  color: #454545;
  background-color: #dfe8f2;
  border-width: 0 1px 0 0;
  border-color: #fff;
  border-bottom: 1px solid #454545;
}

._articleContent table._table thead tr th:last-child {
  border-left:1px!important;
  border-color: #dfe8f2;
  border-bottom: 1px solid #454545
}


._articleContent table._list th{
  text-align: center;
  padding: 9px 0;
  color: #757575;
  font-weight:bold;
  word-break: break-all;
}

._articleContent table._list td {
  text-align: center;
  padding: 9px 5px;
  color: #757575;
  font-weight:bold;
  word-break: break-all;
}

._articleContent table._list tbody tr td:last-child {
  border-right: 1px solid #fff!important;
}


._articleContent table._view{
    border-width: 2px 0 0 0;
    border-style: solid;
    border-color: #333333;

}
._articleContent table._view th{
  text-align: center;
  color: #454545;
  background-color: #e2eaf4;
  font-weight:bold;
  text-align: left;
  font-size: 14px;
  word-break: break-all;
}
._articleContent table._view td{
  color: #757575;
  font-weight:bold;
  word-break: break-all;
}

._articleContent table._view tbody tr td:last-child {
  border-right: 0;/*테이블 마지막 셀 border 제거*/
}
/*(이슈) 5. select 이슈*/
._articleContent table._view tbody tr td select{ 
 vertical-align: top;
}


._articleContent table._view th,
._articleContent table._view td {
  padding:7px 20px;
}
._articleContent table._list th._aL,
._articleContent table._list td._aL {
  padding: 9px;
  text-align: left;
}

/*
 * 테두리상자
 */
._border {
  border-style: solid;
  text-align: center;
  margin-bottom: 30px;
}
._border ._inner {
  display: inline-block;
  zoom: 1;
  *display: block;
}

._border._write {
  border-width: 1px;
  border-color: #dddddd;
  background-color: #f0f0f0;
}
._border._write ._form {
  text-align: left;
  border: 0;
}
._border._write ._form ._insert {
  margin-right: 0;
  width: 920px !important;
  zoom: 1;
  position: relative;
}


/*._border._write ._form ._insert p:after{content:"";display:block;clear:both;width:0px;height:0px}
*/._border._write ._form ._insert table tr td{padding:3px 0;}
._border._write ._form ._insert ul,
._border._write ._form ._insert input[type="text"],
._border._write ._form ._insert input[type="password"],
._border._write ._form ._insert input[type="color"],
._border._write ._form ._insert input[type="date"],
._border._write ._form ._insert input[type="datetime"],
._border._write ._form ._insert input[type="datetime-local"],
._border._write ._form ._insert input[type="email"],
._border._write ._form ._insert input[type="month"],
._border._write ._form ._insert input[type="number"],
._border._write ._form ._insert input[type="range"],
._border._write ._form ._insert input[type="search"],
._border._write ._form ._insert input[type="tel"],
._border._write ._form ._insert input[type="time"],
._border._write ._form ._insert input[type="url"],
._border._write ._form ._insert input[type="week"],
._border._write ._form ._insert .selectboxit-container,
._border._write ._form ._insert ._button,
._border._write ._form ._insert ._fL,
._border._write ._form ._insert label {
  float: left;
  margin: 0 2px;
}
span._fL{display:inline-block; padding-top: 10px;}
._border._write ._form ._insert ._fR{
  float: right;
}
._border._write ._form ._insert label {
  min-width:60px;
  margin-right: 10px;
}


