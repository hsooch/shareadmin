/********** reset ***********/

body, html {
	height: 100%;
}

body * {
	box-sizing: border-box;
}

a {
	color: #333;
	text-decoration: none;
}

a, body, button, h1, h2, h3, h4, h5, h6, html, input, select, strong, table th, textarea {
	font-family: "noto-light", "맑은 고딕", "Malgun Gothic", Dotum, Arial, sans-serif;
	font-size: 16px;
	color: #333;
	letter-spacing: 0;
}

img {
	vertical-align: top;
}

/********** //reset ***********/

/********** input box ***********/
input[type="text"].input-text{
	width:100%;
	height:50px;
	min-width:100px;
	border:1px solid #dedede;
	padding-left:20px;
	font-size:16px;
}

/********** select box ***********/

select.select {
	width:100%;
	height:50px;
	min-width:260px;
	border:1px solid #dedede;
	padding-left:20px;
	font-size:16px;
}
.select-box {
	position: relative;
	width: 200px;
	height: 40px;
	background-color: #fff;
	z-index: 1;
}

.select-box:before {
	content: "";
	position: absolute;
	top: 50%;
	right: 11px;
	width: 10px;
	height: 6px;
	margin-top: -1px;
	background: url("../img/icon/arrow_down.jpg") no-repeat right center;
}

.select-box select {
	width: 100%;
	height: 100%;
	border: 0;
	line-height: normal;
	opacity: 0;
	filter: alpha(opacity=0);
	-webkit-appearance: none;
	-moz-appearance: none;
	appearance: none;
}

.select-box label {
	position: absolute;
	width: 100%;
	height: 100%;
	font-size: 14px;
	color: #333;
	top: 0;
	left: 0;
	z-index: -1;
	display: table;
}

.select-box label span {
	display: table-cell;
	vertical-align: middle;
	padding-left: 10px;
}


