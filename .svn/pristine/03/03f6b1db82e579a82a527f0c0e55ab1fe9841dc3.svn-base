/*[header]*/
header{position:relative;width:100%;z-index:2;}
header .top_area{position:relative;background:#3a55a6;height:60px;}
header .top_area .logo{width:230px;height:60px;position:absolute;left:0;top:0;}
header .top_area .logo a{width:230px;height:60px;display:block;background:url('../images/common/icon/logo.png') 20px center no-repeat;text-indent:-9999px;position:relative;z-index:100;}
header .top_area .pageTit{width:118px;height:24px;margin:18px 0;position:absolute;border-radius:0 12px 12px 0;left:231px;top:0;background:#fff; text-align: center;}
header .top_area .pageTit a{color:#191919;line-height:24px;padding-left: 16px;}
header .top_area .pageTit a:before{content:'';position:absolute;left:14px;top:5px;padding-left:15px;width:16px;height:14px;background:url(../images/common/icon/icon_com.png) no-repeat;}

header .top_area .global_navi{width:100%;min-width:1550px;margin:auto;padding-top:18px;position:relative;z-index:2;text-align:right;}
header .top_area .global_navi > ul{display:inline-block;padding-right:100px;}
header .top_area .global_navi li{float:left;position:relative;margin:0 4px;height:20px;line-height:24px;font-size:12px;}
header .top_area .global_navi li a{color:#fff;display:block;}
header .top_area .global_navi li a span{color:#fffd41;}
header .top_area .global_navi li.radiBtn{ width:87px;height:24px;line-height:24px;text-align:left;background:#fff; border-radius: 12px;}
header .top_area .global_navi li.radiBtn a {color: #333;padding-left:32px;}
header .top_area .global_navi li.gbnv_1{padding-right: 6px; font-size: 13px;}
header .top_area .global_navi li.gbnv_2:before{content:'';position:absolute;left:14px;top:5px;width:18px;height:18px;background:url(../images/common/icon/icon_navibtn.png)	-2px -1px no-repeat;}
header .top_area .global_navi li.gbnv_3:before{content:'';position:absolute;left:14px;top:5px;width:18px;height:18px;background:url(../images/common/icon/icon_navibtn.png) -39px -1px no-repeat;}
header .top_area .global_navi li.gbnv_4:before{content:'';position:absolute;left:14px;top:5px;width:18px;height:18px;background:url(../images/common/icon/icon_navibtn.png) -21px -1px no-repeat;}


/* tab style */
ul.tab_list{position:relative;border-bottom:1px solid #757575;*zoom:1}
ul.tab_list:after{content:"";display:block;clear:both}
ul.tab_list > li{float:left;margin-bottom:-1px;color:#666;background:#f6f6f6;}
ul.tab_list > li a{height:60px;line-height:58px;color:#757575;display:block;border:1px solid #dcdcdc;border-bottom:0;}
ul.tab_list > li a span{display:block;cursor:pointer;font-size:17px;text-align:center;}
ul.tab_list > li.current{color:#333;background:#fff;margin-bottom:-1px;}
ul.tab_list > li.current > a{border:1px solid #757575;border-bottom:1px solid #fff;}
ul.tab_list > li.current > a span{font-weight:bold;}

.tabcontent, .tabcontent h6 {display:none;}
.tabcontent.current, .innertabcontent.innercurrent{display:block;}

.tab_list{position:relative;*zoom:1}
.tab_list:after{content:"";display:block;clear:both}
.tab_list > div{position:relative;min-width:130px;box-sizing:border-box;float:left;color:#666;background:#333333;border-right:1px solid #6d6d6d;}
.tab_list > div .btn_tabclose{position:absolute;right:10px;top:50%;margin-top:-21px;border:0;}
.tab_list > div a{height:40px;line-height:40px;color:#fff;font-size:13px;display:block;margin-right:10px;}
.tab_list > div a span{display:block;cursor:pointer;font-size:13px;text-align:left;margin-left:10px;width:100px;overflow:hidden;white-space:nowrap;text-overflow:ellipsis;}
.tab_list > div.current{color:#fff;background:#2c8bec;}
.tab_list > div.current > a span{font-weight:bold;}
.tab_list > div.current .btn_tabclose{background:url(../images/common/icon/ico_tabclose.png) center center no-repeat; background-position:-12px 15px;}


.tab-content, .tab-content h6{display:none;}
.tab-content.current{display:block;}
.tab-content{padding: 30px 100px 30px 30px;}

.innertab_list{position:relative;}
.innertab_list:before{content:"";position:absolute;width:100%;height:2px;border-bottom:2px solid #3a55a6;top:48px;z-index:10;}
.innertab_list:after{content:"";display:block;clear:both}
.innertab_list > li{position:relative;min-width:150px;box-sizing:border-box;float:left;color:#757575;background:#fff;border:1px solid #d4d4d4;}
.innertab_list > li a{height:50px;line-height:50px;color:#757575;font-size:16px;display:block;margin-right:10px;}
.innertab_list > li a span{display:block;cursor:pointer;font-size:16px;text-align:center;margin-left:10px;font-weight:bold; }
.innertab_list > li.current{color:#fff;background:#445a9c;}
.innertab_list > li.current > a span{font-weight:bold; color: #fff;}

