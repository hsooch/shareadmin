/* tree menu */
.tree{font-size:13px;}
.tree ul{margin:0;padding:0;list-style:none;}
.tree ul ul{margin:0 0 0 -5px;background:url(../images/common/icon/line_tree_first.png) no-repeat 30px 5px;}
.tree li{position:relative;padding:0 0 0 25px;background:url(../images/common/icon/line_tree.png) no-repeat 21px 7px;white-space:nowrap;*zoom:1;}
.tree ul.clfix li.open button.toggle.minus:nth-child(1){background:none;}
.tree>ul.clfix>li.last>button.toggle.minus:first-child{background:transparent url(../images/common/icon/btn_tree.png) no-repeat; background-position:0px -53px;}

.tree > ul > li.last:first-child{background:none;}
.tree li.last,
.tree li.active a{font-weight:bold;color:#333;background: url(../images/common/icon/line_tree_last.png) no-repeat 21px 7px;}
.tree li.active li a{font-weight:normal;color:#767676;}
.tree>ul:first-child>li>a{padding-top:3px;}
.tree a{display:table-cell;padding-top:4px;line-height:21px; color:#7e7f81;text-decoration:none;font-size:13px;padding-left:45px;}
.tree a:hover,
.tree a:active,
.tree a:focus{text-decoration:underline;}
.tree .toggle{overflow:hidden;position:absolute;top:7px;left:21px;width:41px;height:40px;padding:0;border:0;background:transparent url(../images/common/icon/btn_tree.png) no-repeat;cursor:pointer;font-size:0;color:#fff;text-indent:19px;*text-indent:0;vertical-align:middle;}
.tree .toggle.plus{background-position:0 0;}
.tree .toggle.minus{background-position:0px -53px;}


.checkTree li{position:relative;padding:0 0 0 25px;background:url(../images/common/icon/line_checktree.png) no-repeat 21px 7px;}
.checkTree>ul.clfix>li.last>button.toggle.minus:first-child{background:transparent url(../images/common/icon/btn_checktree.png) no-repeat; background-position:0px -53px;}
.checkTree li.last,.checkTree li.active a{background: url(../images/common/icon/line_checktree_last.png) no-repeat 21px 7px;}
.checkTree .toggle{background:transparent url(../images/common/icon/btn_checktree.png) no-repeat;}
