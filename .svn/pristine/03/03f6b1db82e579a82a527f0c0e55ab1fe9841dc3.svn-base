/* cont_bar */
.cont_bar{height:40px;position:relative;background:#333;}
.cont_bar .left_menu{position:absolute;left:0;top:160px;background:#3a4569;border-radius: 0 5px 5px 0;}
.cont_bar .left_menu > p{display:inline-block;}
.cont_bar .left_menu > p > button{padding:0 5px;border:0;}
.cont_bar .left_menu > p > .menuLBtn{min-width: 20px;height:55px;}
.cont_bar .left_menu > p > button.act{opacity:1;
-webkit-transition:opacity 0.3s;
transition:opacity 0.3s;}

.cont_bar .left_menu > p > button ~ button{border-left:1px solid #484848;}
.cont_bar .left_menu > p > button > span{display:block;position:relative;font-size:12px;color:#fff;font-weight:700;line-height:23px;text-indent:-9999px;}
.cont_bar .left_menu > p > button > span:after{content:'';position:absolute;left:0;top:3px;width:12px;height:16px;background:url('../images/common/icon/icon_toggle.png') center center no-repeat;}
.cont_bar .left_menu > p > button.btn_lnm_open > span:after{background:url('../images/common/icon/icon_toggle.png') no-repeat; }
.cont_bar .left_menu > p > button.btn_lnm_close > span:after{background-position: -12px 0;}
.cont_bar .left_menu > ol{display:inline-block;margin-left:24px;}
.cont_bar .left_menu > ol li{display:inline-block;}
.cont_bar .left_menu > ol li > button{width:140px;height:34px;line-height:33px;color:#fff;font-size:14px;font-weight:700;background:#484848;}

.cont_bar .right_menu{position:absolute;height:40px;top:0px;text-align:left;}
.cont_bar .right_menu > p{display:inline-block;vertical-align:top;}
.cont_bar .right_menu > p.err_count{height:34px;background:#fff;padding:0 23px;position:relative;margin-right:5px;}
.cont_bar .right_menu > p.err_count:after{content:'';position:absolute;top:10px;right:-6px;border-width:8px 0 8px 8px;border-style:solid;border-color:transparent;border-left-color:#fff;}
.cont_bar .right_menu > p.err_count > span{height:34px;line-height:33px;padding-left:28px;display:inline-block;position:relative;font-weight:700;}
.cont_bar .right_menu > p.err_count > span:after{content:'';position:absolute;left:0;top:50%;margin-top:-9px;width:24px;height:19px;background:url('../images/common/icon/ico_blk_point.png') no-repeat;}
.cont_bar .right_menu > p > button{height:34px;line-height:33px;color:#fff;font-size:14px;font-weight:700;}
.cont_bar .right_menu > p > button.btn_re-test {background:#c93137;padding:0 20px;}
.cont_bar .right_menu > p > button.btn_re-test span{display:inline-block;padding-left:27px;position:relative;}
.cont_bar .right_menu > p > button.btn_re-test span:after{content:'';position:absolute;left:0;top:50%;margin-top:-7px;width:20px;height:15px;background:url('../images/common/icon/ico_sline.png') no-repeat;}
.cont_bar .right_menu > p > button.btn_list {background:#484848;padding:0 20px;}

/* content_layout */
.content_layout {position:relative;height:100%;padding-bottom:1px;}
.content_layout .cont_left{background:#e8ebf2;border-right:1px solid rgba(212,212,212,.85);width:230px;position:absolute;left:0;top:0;bottom:0;}
.content_layout .cont_right{height:100%;background:#fff;position:relative;margin:0 0 0px 231px;}
.content_layout .cont_right .cont_Title{height:31px;line-height:30px;position:relative;font-size:26px;color:#333;font-weight:500;}
.content_layout .cont_right .btn_RT{position:absolute;right:20px;top:20px;}
.content_layout .cont_left .dragToggle .acco_opened > li > div.hidden_div{padding:0px;}
.content_layout .cont_left .lTitle{text-align:center;padding:32px 0 21px 0;font-size:26px;color:#333;}
.content_layout .cont_left .dragToggle {position:absolute;left:0;right:0;top:74px;bottom:5px;overflow-y:auto;}
.content_layout .cont_left .dragToggle .acco_opened > li {margin-top:-1px;background:#e8ebf1;}
.content_layout .cont_left .dragToggle .acco_opened > li:first-child {border-top:0;}
.content_layout .cont_left .dragToggle .acco_opened > li > div{position:relative;}
.content_layout .cont_left .dragToggle .acco_opened > li > div.opener{position:relative;height:35px;}
.content_layout .cont_left .dragToggle .acco_opened > li > div.opener span{color:#fff;}
.content_layout .cont_left .dragToggle .acco_opened > li > div > span{padding-left:36px;display:block;height:35px;line-height:35px;font-size:13px;color:#757575;font-weight:700;overflow:hidden;white-space:nowrap;text-overflow:ellipsis;cursor:pointer;}
.content_layout .cont_left .dragToggle .acco_opened > li > div.opener{background:#3b4569;color:#fff;}
.content_layout .cont_left .dragToggle .acco_opened > li > div.active{background:#61c6e4;}
.content_layout .cont_left .dragToggle .acco_opened > li > div.act{background:#c93137;}
.content_layout .cont_left .dragToggle .acco_opened > li > div a{position:absolute;left:15px;top:50%;margin-top:-8px;width:18px;height:18px;padding:0;background:url('../images/common/icon/icon_tab.png') 0px 0px no-repeat;}
.content_layout .cont_left .dragToggle .acco_opened > li > div.active a{background-position:0px -16px;}
.content_layout .cont_left .dragToggle .acco_opened > li > div.hidden_div span.acco_dot:before{content:'';position:absolute;left:20px;top:50%;margin-top:-1px;width:3px;height:3px;background:#a8a8a8;border-radius:50%;}
.content_layout .cont_left .dragToggle .acco_opened > li > div.hidden_div span.active:before{background:#484848;}
.content_layout .cont_left .dragToggle .acco_opened > li > div.hidden_div span.active{color:#454545;background:#fff}
.content_layout .cont_left .dragToggle .acco_opened > li > div.hidden_div span.active:after{content:'';position:absolute;right:20px;top:50%;margin-top:-4px;width:8px;height:8px;padding:0;background:url('../images/common/icon/icon_arrow.png') 0px 0px no-repeat;}
.content_layout .cont_left .dragToggle .acco_opened > li > div .option_set{position:absolute;right:6px;top:50%;margin-top:-13px;}
.content_layout .cont_left .dragToggle .acco_opened > li > div.opener .option_set{right:10px;display:none;}
.content_layout .cont_left .dragToggle .acco_opened > li > div.opener:hover .option_set,
.content_layout .cont_left .dragToggle .acco_opened > li > div.opener.active .option_set{display:block;}
.content_layout .cont_left .dragToggle .acco_opened .depth3 li {padding-left:25px;margin-top:-1px;border-top:1px solid #d2d2d2;background:#e8ebf1;cursor:pointer;}
.content_layout .cont_left .dragToggle .acco_opened .depth3 li div{height:35px;line-height:28px;position:relative;}
.content_layout .cont_left .dragToggle .acco_opened .depth3 li div em{width:60px;font-size:13px;color:#757575;vertical-align:middle;display:inline-block;overflow:hidden;white-space:nowrap;text-overflow:ellipsis;}
.content_layout .cont_left .dragToggle .acco_opened .depth3 li:first-child {border-top:0;}
.content_layout .cont_left .dragToggle .acco_opened .depth3 li .option_set{position:absolute;right:3px;top:50%;margin-top:-17px;}

/* accordian - active style */
.acco_opened > li {position:relative;margin-top:-2px;}
.acco_opened > li > div{position:relative;font-size:16px;color:#60461b;position:relative;z-index:1;border-bottom:1px solid #fff;}
