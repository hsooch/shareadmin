/* step 1*/

/* 교류대학목록_20160620 수정 */
.step1-line-wrap .al-left{float:left;}
.step1-line-wrap .al-left ul li{float:left;}
.step1-line-wrap .al-left ul li.bg_gray{background-color:#eaeaea; line-height:40px; text-align:center; margin-right:10px; padding:0 20px;}
.step1-line-wrap .al-left ul li .btn-blue input{
	background-color: #3881df;
	border-radius: 0;
	height: 40px;
	padding: 0 10px;
	min-width: 100px;
	border: 0;
	color: #fff;
	cursor: pointer;
	font-size: 14px;
	margin-left:10px;
	}

.content { width:1200px; margin:0 auto;}
.univ_list .tit_wrap { margin-bottom:80px; text-align: center;}
.univ_list .tit_wrap p,
.univ_list .tit_wrap p strong { font-size:50px; line-height:52px; font-family:"noto-medium"; color:#333; letter-spacing: -1px;}
.univ_list .tit_wrap p { margin-bottom:42px;}
.univ_list .tit_wrap p strong { color:#0f4075;}
.univ_list .tit_wrap span { font-size:20px; line-height:22px; font-family:"noto-regular"; color:#000; letter-spacing: -1px;}
.univ_list .list_area .now_univ { position:relative; margin-top:115px; padding:0 65px 95px 65px;}
.univ_list .list_area .now_univ > p { margin-bottom:80px; text-align: center;}
.univ_list .list_area .now_univ > p strong { position:relative; display:inline-block;  width:320px; padding:26px 0; font-size:28px; line-height:30px; color:#fff; font-family:"noto-regular"; background:url(../images/bg_title.png) center no-repeat; }
.univ_list .list_area .now_univ > p strong::before { position:absolute; left:50%; top:50%; display:block; content:""; width:444px; height:134px; margin-left:-222px; margin-top:-67px; background:url(../images/bg_leaf.png) center no-repeat; opacity:0.3; }
.univ_list .list_area .now_univ .bx-controls { position:absolute; left:0; top:0; width:100%;  height:100%;}
.univ_list .list_area .now_univ .bx-controls .bx-controls-direction{ position:absolute; top:50%; left:0; width:100%; margin-top:-68px;}
.univ_list .list_area .now_univ .bx-controls .bx-controls-direction a { width:40px; height:136px; text-indent: -99999px; overflow:hidden;}
.univ_list .list_area .now_univ .bx-controls .bx-controls-direction .bx-prev{ float:left; background:url(../images/btn_prev.png) no-repeat center;}
.univ_list .list_area .now_univ .bx-controls .bx-controls-direction .bx-next{ float:right; background:url(../images/btn_next.png) no-repeat center;}
.univ_list .list_area .now_univ .bx-controls .bx-pager { position:absolute; bottom:0; left:0; width:100%; text-align:center;}
.univ_list .list_area .now_univ .bx-controls .bx-pager .bx-pager-item { display:inline-block; margin-right:10px;}
.univ_list .list_area .now_univ .bx-controls .bx-pager .bx-pager-item:last-child { margin-right:0px;}
.univ_list .list_area .now_univ .bx-controls .bx-pager .bx-pager-item a { display:block; width:22px; height:22px; text-indent:-99999px; overflow:hidden; background:url(../images/btn_rolling_off.png) no-repeat center;}
.univ_list .list_area .now_univ .bx-controls .bx-pager .bx-pager-item a.active { display:block; width:22px; height:22px; background:url(../images/btn_rolling_on.png) no-repeat center;}
.univ_list .list_area .list_slide li > ul::after { display:block; clear:both; content:"";}
.univ_list .list_area .list_slide li > ul { margin-top:-30px; margin-left:-25px;}
.univ_list .list_area .list_slide li > ul li {float:left; width:25%; overflow:hidden; margin-top:0 !important;}
.univ_list .list_area .list_slide li > ul li p { margin-left:25px; margin-top:30px; padding:20px 0; color:#fff; font-size:22px; line-height:24px; text-align: center; background-size:cover; background-repeat:no-repeat; background-position:center;}
.univ_list .list_area .list_slide li > ul li.blue p { background-image:url(../images/bg_blue.png);}
.univ_list .list_area .list_slide li > ul li.mint p { background-image:url(../images/bg_mint.png);}
.univ_list .list_area .list_slide li > ul li.orange p { background-image:url(../images/bg_orange.png);}
.univ_list .list_area .list_slide li > ul li.gray p { background-image:url(../images/bg_gray.png);}

.univ-selec-mobile{display:none;}
.step1-textarea{
	max-width:1200px;
	width: 100%;
	margin:166px auto 0;
	border:1px solid #e3e3e3;
	border-top:4px solid #0f4075;
	padding:35px 25px;
	height:415px;
	overflow-y:auto;
}
.tit-textarea{
	display: block;
	margin-bottom:30px;
	font-size:24px;
	color:#333;
	font-family: "noto-regular";
}
.step1-textarea .ol-list li{
	font-size:16px;
	line-height: 1.5;
	margin-bottom:10px;
}
.step1-textarea .ol-list li .tx-insert{
	display: block;
	margin-left:85px;
}

@media screen and (max-width:1200px){
    .content { width:100%;}
    /*교류대학목록*/
}
@media screen and (max-width:768px){



    /*교류대학목록*/
    .univ_list{display:none;}
    .univ_list .tit_wrap { margin-bottom:50px;}
    .univ_list .tit_wrap p,
    .univ_list .tit_wrap p strong { font-size:18px; line-height:20px;}
    .univ_list .tit_wrap p { margin-bottom:15px;}
    .univ_list .list_area .now_univ { padding: 0 0 45px 0;}
    .univ_list .list_area .now_univ > p { margin-bottom:25px;}
    .univ_list .list_area .now_univ > p strong { width:160px;  padding:12px 0; font-size:14px; line-height:16px;}
    .univ_list .list_area .now_univ > p strong::before { background-size:50%; }
    .univ_list .list_area .now_univ .bx-controls .bx-controls-direction { display:none;}
    .univ_list .list_area .list_slide li > ul { margin-left:-15px; margin-top:-15px;}
    .univ_list .list_area .list_slide li > ul li { width:50%;}
    .univ_list .list_area .list_slide li > ul li p { margin-left:15px; margin-top:15px; padding:14px 0; font-size:12px; line-height:14px;}
    .univ_list .list_area .now_univ .bx-controls .bx-pager .bx-pager-item { margin-right:5px;}
    .univ_list .list_area .now_univ .bx-controls .bx-pager .bx-pager-item a { width:12px; height:12px; background-size:12px auto;}
    .univ_list .list_area .now_univ .bx-controls .bx-pager .bx-pager-item a.active { width:12px; height:12px; background-size:12px auto;}
	.univ-selec-mobile{display:block;}

	.step1-line-wrap .al-left ul li.add_chk{width:30%}
	.step1-line-wrap .al-left ul li.sel{width:52%}
	.step1-line-wrap .al-left ul li.serch{width:17%}
	.step1-line-wrap .al-left ul li.bg_gray{height:30px; min-width:100px; margin-right:3px; font-size:12px; padding:0 2%; line-height:30px;}
	.step1-line-wrap .al-left ul li .btn-blue input{min-width:100%; height:30px; margin-left:3px;}


	.step1-line-wrap{
		width:100%;
	}
	.step1-line-wrap .al-left{
		float:none;
	}
	.step1-line-wrap ul{
		overflow:hidden;
	}
	.step1-line-wrap ul li{
		float:left;
	}
	.step1-line-wrap ul li:first-child{
		padding-left:0;
	}
	.step1-line-wrap ul li .select{
		width:100%;
		min-width:165px;
	}
	.step1-textarea{
		margin-top:10px;
		border:1px solid #dedede;
		padding:10px;
		height:185px;
	}
	.tit-textarea{
		font-size:12px;
		margin-bottom:10px;
	}
	.step1-textarea .ol-list li{
		font-size:12px;
		margin-bottom:5px;
	}
}

