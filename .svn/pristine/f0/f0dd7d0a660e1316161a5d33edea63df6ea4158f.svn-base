@charset "utf-8";

/* step list */
.step-wrap {
	max-width: 1200px;
	width:100%;
	margin: 0 auto 40px;
}

.step-wrap .myStep-area::after {
	display: block;
	content: "";
	clear: both;
}

.step-wrap .myStep-area {}

.step-wrap .myStep-area li {
	position: relative;
	float: left;
	display: table;
	width:calc(100% / 4);
	height: 100px;
	background-color: #e9e9e9;
}
.step-wrap .myStep-area .step_area{
	position:relative;
	display: table-cell;
	vertical-align: middle;
	width:100%;
	padding-left:56.22%
}
.myStep-area>li .step_area:after{
	content:"";
	display:block;
	position:absolute;
	left:16%;
	top:0%;
	margin-top:10px;
	width:78px;
	height:78px;
	/* transform:translate(0%, -50%); */

}
.step-wrap .myStep1.on .step_area:after {
	background: url(../images/icon_step01_on.png) no-repeat 0 0;
}
.step-wrap .myStep1 .step_area:after {
	background: url(../images/icon_step01.png) no-repeat 0 0;
}
.step-wrap .myStep2 .step_area:after {
	background: url(../images/icon_step02.png) no-repeat 0 0;
}
.step-wrap .myStep3 .step_area:after {
	background: url(../images/icon_step03.png) no-repeat 0 0;
}
.step-wrap .myStep4 .step_area:after {
	background: url(../images/icon_step04.png) no-repeat 0 0;
}
.step-wrap .myStep-area li.on {
	background-color: #3881de;
}
.step-wrap .myStep-area li.myStep4.on:after{
	display: none;
}
.step-wrap .myStep-area li::after {
	position: absolute;
	right: -30px;
	top: 0;
	display: block;
	width: 33px;
	height: 100px;
	content: "";
	background: url(../images/bg_step.png) no-repeat center;
	z-index: 1;
}
.step-wrap .myStep-area li:first-child.on:before{
	display: none;
}
.step-wrap .myStep-area li:last-child::after {
	display: none;
}
.step-wrap .myStep-area li.on::after {
	position: absolute;
	right: -30px;
	top: 0;
	display: block;
	width: 33px;
	height: 100px;
	content: "";
	background: url(../images/bg_step_on.png) no-repeat center;
}
.step-wrap .myStep-area li.on::before {
	position: absolute;
	left: -5px;
	top: 0;
	display: block;
	width: 35px;
	height: 100px;
	content: "";
	background: url(../images/bg_step_before_on.jpg) no-repeat 0 0;
	z-index: 1;
}

.step-wrap .myStep-area .myStep-area1.on::before{
	display: none;
}
.step-wrap .myStep-area li .step_area {
	position: relative;
	background-color: #e9e9e9;
	font-family: "noto-medium";
	color: #999;
}

.step-wrap .myStep-area li .step_area::after {
	display: block;
	content:"";
	margin-top:10px;
	width: 78px;
	height: 78px;
	background-position: center;
	background-size: 78px auto;
	background-repeat: no-repeat;
}
.step-wrap .myStep-area li.on:after{

}
.step-wrap .myStep-area li .step_area .level {
	display: none;
}

.step-wrap .myStep-area li .step_area .step {
	display: inline-block;
	margin-bottom: 16px;
	font-size: 16px;
	line-height: 18px;
}

.step-wrap .myStep-area li .step_area .comment {
	font-size: 20px;
	line-height: 22px;
}

.step-wrap .myStep-area li:last-child .step_area {
	padding-bottom: 5px;
}

.step-wrap .myStep-area li:last-child .step {
	margin-bottom: 5px;
}

.step-wrap .myStep-area li.on .step_area {
	background-color: #3881de;
	color: #fff;
}

@media screen and (max-width:1200px) {
	.step-wrap .myStep-area li .step_area::after,
	.step-wrap .myStep1.on .step_area:after,
	.step-wrap .myStep2.on .step_area:after,
	.step-wrap .myStep3.on .step_area:after,
	.step-wrap .myStep4.on .step_area:after{
		width:60px;
		height:60px;
		background-size:100% 100%;
	}
	.step-wrap .myStep-area li .step_area::after{
		margin-top:17px;
	}
	.step-wrap .myStep-area li .step_area .comment{
		font-size:16px;

	}
}
@media screen and (max-width:768px) {
	.step-wrap{
		min-width:290px;
		overflow:hidden;
		margin-bottom:20px;
	}
	.step-wrap .myStep-area li{
		width:calc(100% / 2);
	}
	.step-wrap .myStep-area li .step_area{
		line-height: 1.2;
	}
	.step-wrap .myStep-area li .step_area::after,
	.step-wrap .myStep1.on .step_area:after,
	.step-wrap .myStep2.on .step_area:after,
	.step-wrap .myStep3.on .step_area:after,
	.step-wrap .myStep4.on .step_area:after{
		width:45px;
		height:45px;
		background-size:100% 100%;
	}
	.step-wrap .myStep-area .step_area{
		padding-left:43%;
	}
	.step-wrap .myStep-area li .step_area::after{
		margin-top:8px;
	}
	.myStep-area>li .step_area:after{
		left:13%;
	}
	.step-wrap .myStep-area li{
		height: 60px;
		margin-bottom:5px;
	}
	.step-wrap .myStep-area li.on::after{
		height: 60px;
		right: -16px;
	}
	.step-wrap .myStep-area li.on::before{
		height: 60px;
		left:-4px;
		background-size:auto 60px;
	}

	.step-wrap .myStep-area li::after{
		height:60px;
		right:-16px;
	}
	.step-wrap .myStep-area li.myStep2:after{
		background:url("../images/bg_step2.jpg") no-repeat 0 0;
		right:-14px;
		background-size:20px 60px;
	}
	.step-wrap .myStep-area li.myStep2.on:after{
		background:url("../images/bg_step2_on.jpg") no-repeat 0 0;
		right:-14px;
		background-size:20px 60px;
	}
	.step-wrap .myStep-area li.myStep3:before{
		display:block;
		content:"";
		width:35px;
		height:60px;
		background:url("../images/bg_step3.jpg") no-repeat 0 0;
		left:0;
		background-size:20px 60px;
		position: absolute;
		left: 0;
		top: 0;
		z-index: 10;
	}
	.step-wrap .myStep-area li.myStep3.on:before{
		display:block;
		content:"";
		width:35px;
		height:60px;
		background: url("../images/bg_step3_on.jpg") no-repeat 0 0;
		left:0;
		background-size:20px 60px;
	}

	.step-wrap .myStep-area li .step_area .step{
		font-size:12px;
		line-height: 1.3;
		margin-bottom:5px;
	}
	.step-wrap .myStep-area li .step_area .comment{
		font-size:12px;
		line-height: 1.3;
	}
	.step-wrap .myStep-area li:last-child .step_area{
		padding-bottom:0;
		line-height: 1.3;
	}

}

