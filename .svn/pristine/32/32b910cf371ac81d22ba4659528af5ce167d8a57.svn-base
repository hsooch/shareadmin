@charset "utf-8";/* button */.btn, button {	padding:0;	margin:0;	box-sizing: border-box;	display: table;	width: auto;	min-width:76px;	height:45px;	padding:0 40px;	text-align: center;	font-family: 'noto-regular', "맑은 고딕", "Malgun Gothic", Dotum, Arial, sans-serif;;	color:#fff;	border:0;	font-size:14px;	letter-spacing: -0.5px;	cursor:pointer;}.btn:hover, button:hover,.btn:active, button:active,.btn:focus, button:focus,.btn:visited, button:visited{	color:#fff;}.btn span, button span{	display:table-cell;	vertical-align: middle;	font-family: 'noto-regular', "맑은 고딕", "Malgun Gothic", Dotum, Arial, sans-serif;;	font-size:inherit;}.btn-full{	width:100%;}.btn-sm{	width:83px;	height:35px;	padding:0 5px;}.btn-lg{	width:224px;	height:65px;	padding:0 5px;	border-radius: 0;	font-size:19px;}.btn-lg span{	font-size:19px;}.btn-fluid{	width:auto;	padding:0 20px;}.btn-epki-login{	width:310px;	height:100px;	font-size:27px;}.btn.btn-epki-login span{	font-size:27px;}.btn-id-login{	width:224px;	height:62px;}.btn-black{	background-color:#000;}.btn-blue{	background-color:#139ef1;}.btn-gray{	background-color:#9f9797;}.btn-radius{	border-radius:8px;}.btn-dark{	background-color:#666666;}.btn-login{	width:100%;	height:62px;	line-height:60px;	text-align: center;	font-size:24px;}.btn-area{	display: table;	margin:40px auto 0;}.btn-area:after{	content:"";	display: block;	clear:both;}.btn-area .btn:first-child{	margin-left:0;}.btn-area .btn{	float:left;	display: table;	margin-left:10px;}.btn-list{	display: table;	width: 100%;	margin-top:40px;}.btn-list li{	display: table-cell;	width: 5%;	text-align: center;	padding-left:10px;}.btn-list li:first-child {	padding-left:0;}.btn-list li .btn{	width: 100%;}/* layout */.intro{	background:url("../images/img_intro.jpg") no-repeat 0 0;	background-size:100% auto;}.wrap{	width:100%;}.intro .wrap{	height: 100%;}.header{	width:100%;	height:60px;	/* line-height:58px; */	background-color:#383334;	text-align: center;}.header .logo{	padding-top:15px;}.header .logo a{	font-size:19px;	color:#fff;	font-family: 'noto-medium', "맑은 고딕", "Malgun Gothic", Dotum, Arial, sans-serif;;	letter-spacing:-1px;}.intro .container{	display: table;	width:100%;	height: 100%;	/* min-height: 1061px; */}.intro .content{	display:table-cell;	vertical-align: middle;}.container{	padding:100px 0;	height:auto;	min-height: 796px;}.footer{	width:100%;	height:90px;	background-color:#333333;	padding:24px 0;}.footer .txt{	text-align:center;}.footer .txt em{	display: block;	font-size:20px;	color:#e0e0e0;	font-family: "noto-medium", "맑은 고딕", "Malgun Gothic", Dotum, Arial, sans-serif;;	margin-bottom:5px;}.footer .txt .copyright{	font-size:14px;	color:#858585;}.ic{	display: inline-block;	vertical-align: middle;}.icon_logo{	width:30px;	height:30px;	background:url("../images/logo.jpg") no-repeat 0 0;	margin-right:8px;}.section-intro{	width:900px;	height:520px;	margin:0 auto;	background:rgba(225, 225, 225, 0.7);	border-radius:10px;	padding:66px 90px 25px;	position:relative;}.section-intro:after{	content:"";	display:block;	clear:both;}.section-intro .box{	width:calc(100% / 2);	float:left;	text-align: center;	min-height:350px;	overflow:hidden;}.login-epki{	border-right:1px solid #d4d4d4;}.login-epki .radio-list{	display: inline-block;	margin:20px auto 0;}.login-epki .radio-list dt{	margin-right:26px;}.login-epki .radio-list dt,.login-epki .radio-list dd{	float:left;	vertical-align: middle;}.login-epki .radio-list dd{	margin-left:20px;}.login-epki .radio-list dd:first-child{	margin-left:0;}.login-epki .radio-list dt,.login-epki .radio-list dd,.login-epki .radio-list dd label{	font-size:14px;	font-family: "noto-regular", "맑은 고딕", "Malgun Gothic", Dotum, Arial, sans-serif;;	line-height: 1;}.login-epki .radio-list dd label{	display: inline-block;	margin-left:10px;}.login-epki .btn-epki-login{	margin:0 auto;}.login-epki .btn-list{	width:310px;	margin-left: auto;	margin-right: auto;}.btn-list li .btn{	padding:0 10px;}.section-intro .box .title-login{	font-size:38px;	display: block;	margin-bottom:20px;	text-align: center;}.section-intro .login-id .inner{	width:310px;	margin:0 auto;}.section-intro .login-id .form-control{	height:45px;	background-color:#fafec0;	font-size:14px;}.section-intro .login-id .checkbox{	font-size:14px;	text-align: left;	line-height: 1;	vertical-align:middle;}.section-intro .login-id .btn-list{	margin-top:10px;}.section-intro .txt{	display: block;	width: 100%;	position:absolute;	bottom:30px;	left:0;	color:#333;	text-align: center;}.section-intro .txt>span{	color:#335ee0;}/* sub */body{	background-color:#fbfbfb;}h2.title{	position:relative;	padding-top:15px;	font-size:24px;	color:#333333;	font-family: "noto-medium", "맑은 고딕", "Malgun Gothic", Dotum, Arial, sans-serif;;	margin-bottom:26px;}h2.title:before{	position:absolute;	left:0;	top:0;	content:"";	display: block;	width:20px;	height:5px;	background-color:#139ef1;}.section-wrap{	width:670px;	margin:0 auto;}.location-nav{	margin-bottom:25px;}.location-nav:after{	content:"";	display: block;	clear:both;}.location-nav li{	position:relative;	font-size:14px;	float:left;	margin-right:10px;}.location-nav li.home:before{	content:"";	display: inline-block;;	width:11px;	height:10px;	background:url("../images/icon_home.jpg") no-repeat 0  0;	margin-right:18px;}.location-nav li:after{	content:">";	display: inline-block;	margin-left:10px;	font-family: "noto-light", "맑은 고딕", "Malgun Gothic", Dotum, Arial, sans-serif;;}.location-nav li.this{	font-family: "noto-medium", "맑은 고딕", "Malgun Gothic", Dotum, Arial, sans-serif;}.location-nav li.this:after{	display: none;}.step-list:after{	content:"";	display: block;	clear:both;}.step-list li{	float:left;	width:calc(100% / 4);}.step-list li .inner{	overflow: hidden;}.step-list li .inner .img{	 float:left;}.step-list li .inner .txt{	margin-left:85px;}.step-list li .inner .txt .tx-step{	display: block;	font-size:16px;	color:#878787;	font-family: "noto-regular", "맑은 고딕", "Malgun Gothic", Dotum, Arial, sans-serif;}.step-list li .inner .txt span{	display: block;	margin-top:10px;	color:#333333;	font-size:14px;	font-family: "noto-regular", "맑은 고딕", "Malgun Gothic", Dotum, Arial, sans-serif;}.step-list li.on .inner .txt .tx-step{	color:#139ef1;}.content-wrap{	margin-top:50px;}.content-wrap > .inner{	background-color:#fff;	border:1px solid #e1e1e1;	border-top:3px solid #20a3f1;	padding:25px 20px;;}.title-sub-info{	font-size:24px;	color:#3a55a6;	font-family:"noto-medium", "맑은 고딕", "Malgun Gothic", Dotum, Arial, sans-serif;	margin-bottom:25px;}.form-control{	height:31px;	border-radius:0;	border-color:#e3e3e3;	padding-top:3px;	padding-bottom:3px;}.tb+.title-sub-info{	margin-top:33px;}.tb{	width:100%;	/* margin-top:33px; */}.tb tbody tr:first-child th,.tb tbody tr:first-child td{	border-top:0;}.tb tbody tr th,.tb tbody tr td{	border-top:1px solid #e3e3e3;	padding:20px 0;	vertical-align: top;}.tb tbody tr td{	padding-left:30px;}.tx-box{	padding:10px;	border:1px solid #bad3fb;	background-color:#eff7f9;	font-size:13px;	margin-top:20px;	line-height:1.4;}.tx-box span{	font-weight: bold;}.row .tx{	text-align: center;	float:left;}.row .col-xs-2 .btn{	height:30px;	line-height:28px;	border-radius:0;}.confirm-wrap{}.title-sub-confirm{	font-size:32px;	font-family: "noto-regular", "맑은 고딕", "Malgun Gothic", Dotum, Arial, sans-serif;	margin-bottom:25px;}.confirm-wrap .sub-txt{	font-size:16px;	font-family: "noto-regular", "맑은 고딕", "Malgun Gothic", Dotum, Arial, sans-serif;	line-height: 1.2;}.confirm-box{	padding-left:280px;	background:url("../images/icon_security.jpg